<div class="container">
  <div class="mb-4 mt-5">
    <h3>会員一覧</h3>
  </div>
  <table class = "table">
    <thead class ="table-dark">
      <tr>
        <td>会員ID</td>
        <td>氏名</td>
        <td>メールアドレス</td>
        <td>ステータス</td>
      </tr>
    </thead>
    <tbody>
      <% @end_users.each do |end_user| %>
      <tr>
        <td><%= end_user.id %></td>
        <td>
          <%= link_to admin_end_user_path(end_user), class: "text-dark" do %>
          <%= end_user.last_name %><%= end_user.first_name %>
          <% end %>
        </td>
        <td><%= end_user.email %></td>
        <td>
          <% if end_user.is_deleted == false %>
            <span class="text-success">有効</span>
          <% else %>
            <span class=text-secondary>退会</span>
          <% end %>
        </td>
      <% end %>
      </tr>
    </tbody>
  </table>
<%= paginate @end_users %>
</div>