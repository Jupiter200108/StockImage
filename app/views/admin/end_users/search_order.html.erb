<div class="container">
  <div class="row">
    <div class="col-12 px-0 mx-auto">
      <div class="d-flex bd-highlight mb-3">
       <h4><strong><%= @end_user.name %>さんの注文履歴一覧</strong></h4>
     </div>
<table class="table border">
  <thead>
    <tr>
      <th class="table-active table-dark">購入日時</th>
      <th class="table-active table-dark">購入者</th>
    </tr>
  </thead>
  <tbody>
    <% @end_userorders.each do |order| %>
    <tr>
      <td>
        <%= link_to admin_order_path(order) do %>
          <%= order.created_at.strftime("%Y-%m-%d %H:%M:%S") %>
        <% end %>
      </td>
      <td><%= order.end_user.name %></td>
    </tr>
    <% end %>
   </tbody>
  </table>
  </div>
 </div>
</div>